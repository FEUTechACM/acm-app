{
	"name": "feu-tech-acm",
	"version": "0.1.0",
	"private": true,
	"description": "The FEU Tech ACM's official and premier cross-platform application.",
	"keywords": [
		"FEU Tech",
		"ACM",
		"Student Chapter",
		"FEU Tech ACM",
		"FEU Tech ACM Student Chapter",
		"Application",
		"Web Application",
		"FEU Tech ACM-X",
		"FEU Tech ACM-X Application",
		"FEU Tech ACM-X Web Application",
		"FEU Tech ACM-X Web Application",
		"Official Application",
		"Google Play Store",
		"Website"
	],
	"scripts": {
		"dev": "next dev",
		"devx": "next dev --turbo",
		"build": "yarn run partytown && next build",
		"postbuild": "next-sitemap",
		"partytown": "partytown copylib public/~partytown",
		"start": "next start",
		"analyze": "ANALYZE=true yarn build",
		"lint": "next lint && stylelint \"**/*.css\"",
		"prepare": "husky install",
		"bubblewrap": "npx bubblewrap init --manifest=https://acmx.vercel.app/manifest.webmanifest",
		"killport": "node -e \"const kill = require('kill-port');const p = 3000; setTimeout(() => kill(p, 'tcp').then(console.log('killed 3000'),process.exit(0)).catch(console.log('3000 not killed'),process.exit(0)), 1000)\"",
		"removeNext": "node -e \"const fs = require('fs');const p = '.next';try {fs.rmdirSync(p, { recursive: true });} catch (e) {console.warn('Error folder:', p, e);}process.exit(0);\"",
		"unlighthouse": "npx unlighthouse  --site http://localhost:3000",
		"prisma-reset": "npx prisma db push --force-reset && yarn prisma db seed"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"dependencies": {
		"@auth/prisma-adapter": "^1.1.0",
		"@bubblewrap/cli": "^1.21.0",
		"@builder.io/partytown": "^0.9.0",
		"@next/bundle-analyzer": "^14.1.0",
		"@nextui-org/react": "^2.2.9",
		"@prisma/client": "^5.8.1",
		"@prisma/extension-accelerate": "^0.6.2",
		"@reduxjs/toolkit": "^2.1.0",
		"@t3-oss/env-nextjs": "^0.8.0",
		"@types/node": "20.11.10",
		"@types/react": "18.2.48",
		"@types/react-dom": "18.2.18",
		"@vercel/analytics": "^1.1.2",
		"eslint": "8.56.0",
		"eslint-config-next": "14.1.0",
		"framer-motion": "^11.0.3",
		"next": "14.1.0",
		"next-auth": "^4.24.5",
		"next-pwa": "^5.6.0",
		"next-redux-wrapper": "^8.1.0",
		"next-sitemap": "^4.2.3",
		"nodemailer": "^6.9.8",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hook-form": "^7.50.0",
		"react-redux": "^9.1.0",
		"sharp": "0.33.2",
		"tsparticles": "^3.1.0",
		"typescript": "^5.3.3",
		"zod": "^3.22.4"
	},
	"main": "next.config.js",
	"repository": "git@github.com:FEU-Tech-ACM-Student-Chapter/acm-app.git",
	"author": "Alpha Romer Coma <alpha.coma.ict@gmail.com> (https://linkedin.com/in/alpharomercoma)",
	"license": "Apache-2.0",
	"devDependencies": {
		"@commitlint/cli": "^18.6.0",
		"@commitlint/config-conventional": "^18.6.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@tsparticles/engine": "^3.1.0",
		"@tsparticles/react": "^3.0.0",
		"@types/adm-zip": "^0.5.5",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/next-pwa": "^5.6.9",
		"@types/nodemailer": "^6.4.14",
		"@types/sharp": "^0.32.0",
		"@vercel/speed-insights": "^1.0.8",
		"adm-zip": "^0.5.10",
		"autoprefixer": "^10.4.17",
		"critters": "^0.0.20",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"husky": "^9.0.6",
		"kill-port": "^2.0.1",
		"lint-staged": "^15.2.0",
		"next-compose-plugins": "^2.2.1",
		"pdf-lib": "1.17.1",
		"postcss": "^8.4.33",
		"prettier": "^3.2.4",
		"prisma": "^5.8.1",
		"react-icons": "^5.0.1",
		"react-share": "^5.0.3",
		"semantic-release": "^23.0.0",
		"stylelint": "^16.2.0",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-config-recommended": "^14.0.0",
		"stylelint-order": "^6.0.4",
		"tailwindcss": "^3.4.1",
		"tsx": "^4.7.0"
	},
	"stylelint": {
		"extends": [
			"stylelint-config-recommended"
		],
		"rules": {
			"at-rule-empty-line-before": [
				"always",
				{
					"except": [
						"blockless-after-blockless",
						"blockless-after-same-name-blockless",
						"first-nested"
					],
					"ignore": [
						"after-comment",
						"inside-block"
					],
					"ignoreAtRules": [
						"apply",
						"screen",
						"font-face",
						"nest"
					]
				}
			],
			"at-rule-no-unknown": [
				true,
				{
					"ignoreAtRules": [
						"tailwind",
						"variants",
						"responsive",
						"apply",
						"screen"
					]
				}
			],
			"property-no-unknown": [
				true,
				{
					"ignoreProperties": [
						"font-path"
					]
				}
			],
			"selector-nested-pattern": "^&"
		},
		"ignoreFiles": [
			"public/2023/media/icons/**/*"
		]
	},
	"plugins": [
		"@semantic-release/commit-analyzer",
		"@semantic-release/release-notes-generator",
		"@semantic-release/changelog",
		"@semantic-release/github",
		"@semantic-release/npm",
		"@semantic-release/git"
	],
	"release": {
		"prepare": [
			"@semantic-release/changelog",
			"@semantic-release/npm",
			{
				"path": "@semantic-release/git",
				"assets": [
					"package.json",
					"package-lock.json",
					"CHANGELOG.md"
				],
				"message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
			}
		]
	},
	"browser": {
		"original-fs": false
	}
}
